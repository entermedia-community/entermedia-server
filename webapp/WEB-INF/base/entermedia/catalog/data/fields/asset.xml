<properties>

	<!-- asset details -->
	<property id="id" index="true" keyword="true" stored="true">Id</property>
	<property id="name" index="true" stored="true" keyword="true" editable="false">File Name</property>
	<property id="name_sortable" >File Name</property>
	<property id="catalogid" viewtype="list" catalogid="entermedia" listid="catalogs">Catalog</property>
	<property id="description">Search Text</property>
	<property id="sourcepath" index="true" stored="true">Source Path</property>
	<property id="category" datatype="textjoin" externalid="assetcategory.categoryid" query="assetcategory.assetid = asset.id">Categories</property>
	<property id="relatedasset" index="true" stored="true" datatype="textjoin" externalid="assetrelated.parentid" query="assetrelated.assetid = asset.id">Related Assets</property>
	<property id="fulltext" keyword="true">Full Text</property> 	<!-- This is for PDFs -->
	<property id="assetviews" datatype="number" viewtype="number" index="true" stored="true" >Views</property> 	<!--  These are writen to by outside API -->
	<property id="assetviews_sortable" datatype="number" >Views</property>
	<property id="assetvotes"  datatype="number" index="true" stored="true" >Vote Rank</property>
	<property id="assetvotes_sortable" datatype="number" >Vote Count</property>
	<property id="length" index="true" stored="true" datatype="number">Length(seconds)</property> <!-- Videos -->
	<property id="framerate" index="true" stored="true" datatype="text" externalid="VideoFrameRate">Frame rate</property>
	<property id="bitspersample" externalid="BitsPerSample" index="true" stored="true" datatype="text">Bits Per Sample</property> 	<!-- IPTC/EXIFF/XMP -->
	<property id="compression" externalid="Compression" index="true" stored="true" datatype="text">Compression</property>
	<property id="cammaker" externalid="Make" index="true" stored="true" datatype="text">Camera Maker</property>
	<property id="cammodel" externalid="Model" index="true" stored="true" datatype="text">Camera Model</property>
	<property id="artist" externalid="Artist" index="true" stored="true" datatype="text">Artist</property>
	<property id="comment" externalid="Comment,UserComment" index="true" stored="true" datatype="text">Comment</property>
	<property id="contributor" externalid="Contributor" index="true" stored="true" datatype="text">Contributor</property>
	<property id="creator" externalid="Creator" index="true" stored="true" datatype="text">Creator</property>
	<property id="source" externalid="Source" index="true" stored="true" datatype="text">Source</property>
	<property id="iso" externalid="ISO" index="true" stored="true" datatype="text">ISO</property>
	<property id="imageorientation" index="true" stored="true" datatype="list" editable="true" listid="imageorientation" externalid="Orientation">Image Rotation</property>
	<property id="author" externalid="Author" index="true" stored="true" datatype="text">Author</property>
	<property id="localpath" index="true" stored="true" editable="false">Local Path</property> <!-- Uploading -->
	<property id="fileformat" index="true" stored="true" viewtype="list" editable="true" externalid="FileType">File Format</property>
	<property id="filesize" index="true" stored="true" datatype="number" viewtype="filesize" externalid="FileSize">File Size</property>
	<property id="colortype" index="true" stored="true" viewtype="list" datatype="list">Color</property>
	<property id="width" index="true" stored="true" datatype="number" >Width</property>
	<property id="height" index="true" stored="true" datatype="number" >Height</property>
	<property id="assetcreationdate" editable="true" index="true" stored="true" datatype="date"  externalid="Create Date,MediaCreateDate,CreateDate">Created On</property>
	<property id="assetmodificationdate" editable="false" index="true" stored="true" datatype="date" viewtype="time" >Last Modified</property>
	<property id="recordmodificationdate" index="true" type="date" stored="true" editable="false">Record Date</property> <!--  should be a date -->
	<property id="assetaddeddate" editable="true" index="true" stored="true" datatype="date" viewtype="time" externalid="Record Creation Date">Uploaded Date</property>
	<property id="owner" index="true" viewtype="user" listcatalogid="system" listid="user" keyword="true" stored="true" editable="true" query="groups:developers">Owner</property>
	<property id="primaryfile" index="true" stored="true">Primary File Name</property>
	<property id="cumulusid" index="true" stored="true">Cumulus ID</property>
	
	<property id="importstatus" index="true" stored="true" datatype="list" listid="asset/importstatus" editable="true">Import Status</property>
	<property id="previewstatus" index="true" stored="true" editable="true" viewtype="list" listid="asset/previewstatus" datatype="list">Preview Status</property>
	<property id="editstatus" index="true" stored="true" editable="true" viewtype="list" listid="asset/editstatus" datatype="list">Edit Status</property>

	<!-- Asset Contains -->
	<property id="assettitle" index="true" stored="true" editable="true" datatype="text" keyword="true" listchild="productionnumber" externalid="By-lineTitle,Title,Headline" >Title</property>
	<property id="caption" index="true" stored="true" keyword="true" editable="true" externalid="ImageDescription,Caption/Abstract">Caption</property>
	<property id="longcaption"  keyword="true" editable="true">Long Caption</property>
	<!--  this is built in, Used for related searches property id="keywords" keyword="true" index="true" stored="true" editable="false" >Tags</property --> 
	<property id="extrakeywords" keyword="true" index="true" stored="true" editable="true" externalid="Keywords">Extra Keywords</property> 
	<property id="purpose" index="true" stored="true" editable="true" viewtype="list" datatype="list">Purpose</property>
	<property id="ethnicity" datatype="list" editable="true" index="true" stored="true" viewtype="list" listchild="ethnicitydetailed" >General Ethnicity</property>
	<property id="ethnicitydetailed" foreignkeyid="ethnicity" datatype="list" index="true" stored="true" editable="true" viewtype="list" >Specific Ethnicity</property>
	<property id="viewpoint" datatype="list" editable="true" index="true" stored="true" >Asset Viewpoint</property>
	<property id="region" datatype="list" editable="true" index="true" stored="true" viewtype="list" listchild="country" >Region</property>
	<property id="country" foreignkeyid="region" datatype="list" index="true" stored="true" editable="true" viewtype="list" listchild="state" >Country</property>
	<property id="state" foreignkeyid="country" datatype="list" index="true" stored="true" editable="true" viewtype="list" >State</property>
	<property id="royaltyfree" datatype="boolean" index="true" stored="true" editable="true">Royalty Free</property>
	<property id="publisheds3" datatype="boolean" index="true" stored="true" editable="true">Published S3</property>
	<property id="language" index="true" stored="true" editable="true" viewtype="list" datatype="list" sort="textUp" >Language</property>
	<property id="stocknumber" index="true" keyword="true" editable="true" stored="true">Stock Number</property>
	<property id="jobnotes" keyword="true" editable="true" index="true" stored="true" externalid="Notes">Job Notes</property>
	<property id="episode" keyword="true" editable="true" index="true" stored="true" >Episode</property>

	<!-- Approvals -->	
	<!--property id="assetclients.clientid" datatype="textjoin" index="true" stored="true" listid="clients" query="assetclients.assetid = asset.id">Asset Clients</property -->
	<!-- Geo Location -->
	<property id="position" type="position" datatype="position" viewtype="map" index="true" stored="true">Position</property>

	<property id="assettype" datatype="list" editable="true" index="true" stored="true" viewtype="list"  >Asset Type</property>
	<property id="title" datatype="list" editable="true" index="true" stored="true" viewtype="list"  listchild="productionnumber">Title</property>
	<property id="productionnumber" datatype="list" editable="true" index="true" stored="true" viewtype="list"  >Production Number</property>
	
	<property id="fatwireid" index="true" stored="true">FatWire ID</property>


</properties>
